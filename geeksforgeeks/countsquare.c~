#include<stdio.h>
#include<math.h>
int main()
{
int i,t,r;

long long unsigned int n;
long double root,s;


scanf("%d",&t);
	for(i=0;i<t;i++)
	{
		scanf("%llu",&n);
		root=sqrt(n);
		r= (int)sqrt(n);
		s= r*(1.0);
		
		
		
		if(root==s)
		{
			printf("%d\n",(int)(root-1));
			
		}
		else if(root!=s)
		{
			printf("%d\n",(int)root);
		}
	}
return 0;
}
		
		
			
		
		

