#include<stdio.h>
#include<string.h>
int main()
{
	int i,t,d,length;
	char str[106];
	scanf("%d",&t);
	
	for(i=0;i<t;i++)
	{
		scanf("%s",str);
		
		length = strlen(str);
		
		d = str[length-2]*10 + str[length-1];
		
		if(d%4 == 0)
		{
			printf("%d\n",1);
		}
		else
		{
			printf("%d\n",0);
		}
	}
	return 0;
}
