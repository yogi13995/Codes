#include<stdio.h>

int main()
{
	int i,t,n,fact,j;
	scanf("%d",&t);
	
	for(i=0;i<t;i++)
	{
		scanf("%d",&n);
		fact=1;
		j=1;
		
		while(fact<n)
		{
			fact = fact*j;
			
			
			j++;
		}
		
		if(fact == n)
			{
				printf("%d\n",1);
			}
			else if(fact>n)
			{
				printf("%d\n",0);
			}
	}
return 0;
}
