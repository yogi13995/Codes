#include<stdio.h>

int main()
{
int i,d,r,a,n,t;
scanf("%d",&t);
	for(i=0;i<t;i++)
	{
		scanf("%d",&n);
		a=n;
		d=1;
		while(d!=0)
		{
			d=a/2;
			r=a%2;
			
		    if(n==1 || (d==1 && r==0))
			{
				printf("True\n");
				break;
			}
			else if(r!=0)
			{
				printf("False\n");
				break;
			}
			
			a=d;
		}
	}
return 0;
}
			
			
