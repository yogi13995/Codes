#include<stdio.h>

int main()
{
long long int b[100],c,d,e,f,sum,k,m,l;
int a ,i,j;
scanf("%d",&a);

for(i=0;i<a;i++)
{
scanf("%lld",&b[i]);
f=10;
sum=0;
for(j=0;j<12;j++)
{
l=10^j;
m=b[i]%f;
for(k=0;k<j;k++)
{

e=m/l;

printf("%lld\n",l);
sum=sum+e;
l=l/10;
if(e==0)
{
break;
}


}

f=f*10;
}
}
return 0;
}



