#include<stdio.h>

int main()
{
	int i,t,n,b,c,a,j;
	scanf("%d",&t);
	for(i=0;i<t;i++)
	{
		scanf("%d",&n);
		b=1;
		c=0;
		j=0;
		a=0;
		
		while(a<=n)
		{
			a=b+c;
			c=b;
			b=a;
			if(a==n)
			{
				printf("Yes\n");
				break;
			}
			else if(a>n)
			{
				printf("No\n");
			}
			j++;
		}
	}
return 0;
}
		
			
